OPENAI_API_KEY=your_openai_api_key_here
PORT=5200
NODE_ENV=development
LOG_LEVEL=info
ANTHROPIC_API_KEY=your_anthropic_key_here    # optional (future multi-provider)
BMP_FILTER=0                                 # set to 1 to enable BMP filtering heuristics
KEEP_ALIVE=0                                 # dev override to keep process alive on signals
GIT_SHA=                                     # injected at build/deploy (short commit hash)
BUILD_TIME=                                  # injected ISO8601 build timestamp
GOAL_MIN=0.9                                 # accuracy thresholds for evaluation scripts
BMP_MIN=0.9
ACTIVITY_MIN=0.9
METRIC_MIN=0.9
METRIC_VALUE_TOLERANCE=0.01
